   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   <context:annotation-config></context:annotation-config>
   <bean id="st" class="com.Student"  >
   		<property name="id" value="7"></property>
   		<property name="name" value="Nitish"></property>
   		<!-- <constructor-arg ref="adr"></constructor-arg> -->
   </bean>
   <bean id="arr1" class="com.Address">
		<property name="dNo" value="77"></property>
		<property name="city" value="Kanipakam"></property>
   </bean>
   <bean id="arr2" class="com.Address">
		<property name="dNo" value="777"></property>
		<property name="city" value="Tirupati"></property>
   </bean>
   
   </beans>